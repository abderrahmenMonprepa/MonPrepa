<p id="notice"><%= notice %></p>

<h1>Documents</h1>

<table>
  <thead>
    <tr>
      <th>Document key</th>
      <th>Document section</th>
      <th>Document level</th>
      <th>Document course</th>
      <th>Document type</th>
      <th>Document subject</th>
      <th>Document year</th>
      <th>Document data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_documents.each do |document| %>
      <tr>
        <td><%= document.document_key %></td>
        <td><%= document.document_section %></td>
        <td><%= document.document_level %></td>
        <td><%= document.document_course %></td>
        <td><%= document.document_type %></td>
        <td><%= document.document_subject %></td>
        <td><%= document.document_year %></td>
        <td><%= document.document_data %></td>
      </tr>
    <% end %>
  </tbody>
</table>

