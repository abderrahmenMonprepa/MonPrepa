      <%= stylesheet_link_tag    'font-awesome' %>
      <%= stylesheet_link_tag    'themify-icons' %>
      <%= stylesheet_link_tag    'animate' %>
      <%= stylesheet_link_tag    'theme' %>
      <%= stylesheet_link_tag    'responsive' %>
