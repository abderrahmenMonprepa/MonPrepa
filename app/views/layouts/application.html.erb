    <title>MonPrepa</title>
    <%= csrf_meta_tags %>

    
    <% if controller_name != 'documents' and controller_name != 'registrations' and controller_name != 'users' %>
     <!-- Load JS files -->
      
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
