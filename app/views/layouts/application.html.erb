    <% else %>
      <!-- Load JS files -->
      <%= javascript_include_tag 'application' %>
      <%= javascript_include_tag('popper.min') %>
      <%= javascript_include_tag('bootstrap_elegent') %>
      <%= javascript_include_tag('jquery.sparkline.min') %>
      <%= javascript_include_tag('jquery.easing.min') %>
      <%= javascript_include_tag('register-init') %>           
      <%= javascript_include_tag('perfect-scrollbar.jquery.min') %>
      <%= javascript_include_tag('custom') %>
      <%= javascript_include_tag('d3.min') %>
      <%= javascript_include_tag('morris') %>
      <%= javascript_include_tag('c3.min') %>
      <%= javascript_include_tag('dashboard1') %>
      <%= javascript_include_tag('jquery.toast') %>      
      <%= javascript_include_tag('raphael-min') %>
      <%= javascript_include_tag('sidebarmenu') %>
      <%= javascript_include_tag('waves') %>
      

      <!-- Load StyleSheets files -->
      <%= stylesheet_link_tag  'application', media: 'all' %>
      <%= stylesheet_link_tag    'c3.min' %>
      <%= stylesheet_link_tag    'cryptocoins' %>
      <%= stylesheet_link_tag    'dashboard1' %>
      <%= stylesheet_link_tag    'flag-icon.min' %>
      <%= stylesheet_link_tag    'font-awesome.min' %>
      <%= stylesheet_link_tag    'jquery.toast' %>
      <%= stylesheet_link_tag    'materialdesignicons.min' %>
      <%= stylesheet_link_tag    'morris' %>
      <%= stylesheet_link_tag    'simple-line-icons' %>
