      <%= javascript_include_tag 'owl.carousel.min' %>
      <%= javascript_include_tag 'nivo-lightbox.min' %>
      <%= javascript_include_tag 'modernizr.custom' %>
      <%= javascript_include_tag 'jquery.validate.min' %>
      <%= javascript_include_tag 'jquery.counterup.min' %>
      <%= javascript_include_tag 'jquery.countdown.min' %>
      <%= javascript_include_tag 'jquery.ajaxchimp.min' %>
      <%= javascript_include_tag 'prepa_custom' %>
      <%= javascript_include_tag 'bootstrap-dialog.min' %>

      <!-- Load StyleSheets files -->
 
      <%= stylesheet_link_tag  'application', media: 'all', 'data-turbolinks-track': 'reload' %>
